{
  "version": 2,
  "builds": [
    {
      "src": "faq2.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "faq2.py"
    }
  ],
  "env": {
    "COHERE_API_KEY": "@cohere_api_key",
    "PINECONE_API_KEY": "@pinecone_api_key",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}
